# Social Media API Keys
# Copy this file to .env and fill in your actual API keys

# Facebook/Meta Platform
VITE_FACEBOOK_APP_ID=your_facebook_app_id_here
VITE_FACEBOOK_APP_SECRET=your_facebook_app_secret_here

# Twitter/X Platform
VITE_TWITTER_API_KEY=your_twitter_api_key_here
VITE_TWITTER_API_SECRET=your_twitter_api_secret_here
VITE_TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here

# LinkedIn Platform
VITE_LINKEDIN_CLIENT_ID=your_linkedin_client_id_here
VITE_LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret_here

# Application Settings
VITE_APP_URL=http://localhost:5173
VITE_API_BASE_URL=https://api.yourdomain.com

# Security Settings
VITE_ENCRYPTION_KEY=your_32_character_encryption_key_here
VITE_JWT_SECRET=your_jwt_secret_key_here

# Development Settings
NODE_ENV=development
VITE_DEBUG_MODE=true